                    <!--<span class="bar"></span>-->
                    <label>Enter Your Mobile Number</label>
                </div>

                <div class="group">
                    <input type="password" id="txtname"  name="password" maxlength="25" data-rule-required="true" data-msg-required="Please enter password">
                    <!--<span class="highlight"></span>-->
                    <!--<span class="bar"></span>-->
                    <label>Enter Your Password</label>
                </div>
                <button class="new-sign-in" type="submit">Login</button>
               
                <p class="already f-color f-get-pass">Forgot Password</p>
                <p class="already">Are you a new user?</p>
                <div class="already-log-in click-sign">Sign Up</div>
            </form>
        </div>
