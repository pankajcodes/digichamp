                                    </li>

                                    <li>
                                        <a href="profile.html" class="last_info">See all notifications</a>
                                    </li>
                            </ul>
                        </li>*@
                        <li class="ntspl-dropdown dropdown">
                            <!-- Change design here, its bit of odd or not upto usable -->
                            <a href="#" class="dropdown-toggle avatar_width" data-toggle="dropdown" role="button" aria-expanded="false">
                                <span class="avatar-holder">
                                    @{
                                        string name = string.Empty;
                                        string img = string.Empty;
                                        if (Session["USER_CODE"] != null)
                                        {
                                            int sid = Convert.ToInt32(Convert.ToString(Session["USER_CODE"]).Substring(1));
                                            var student = DbContext.tbl_DC_Registration.Where(x => x.Regd_ID == sid).FirstOrDefault();
                                            if (student != null)
                                            { 
                                            name = Convert.ToString(student.Praent_Name);
                                            }
                                        }
                                    }
									
									  @if (img == null || img == "")
                                    {
                                        <text><img src="~/Student_assets/assets/images/avatar/one.png" alt="" /></text>
                                    }
                                    else
                                    {
                                        <text><img src="~/images/Profile/@img" alt=""></text>
                                    }
                                    @*@if (img == null || img == "")
                                    {
                                        <text><img src="~/Student_assets/assets/images/avatar/one.png" alt="" /></text>
                                    }
                                    else
                                    {
                                        <text><img src="~/images/Profile/@img" alt=""></text>
                                    }*@
                                </span>
                                <span class="avatar_info">
                                    @if (name != "")
                                    {
                                        <text>@name</text>
                                    }
                                </span><span class="drop-icon"></span>
                            </a>
                            <ul class="dropdown-menu login-arrow dropdown-ntspl-menu  animated fadeInUp wow avatar_drop neat_drop dropdown-right" data-wow-duration="1500ms" role="menu">
                                
                                <li>
                                    <a href="/Parent/ChangePassword"> <i class="ion-android-create"></i>Change Password</a>
                                </li>
                                <li>
                                    <a href="/Parent/Logout" class="logout_button"><i class="ion-power"></i>Logout</a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </nav>
            </div>

            <!-- Page Header -->
            <div class="page_header page_header_tab">
                <div class="row">
                    <div class="col-md-8 pull-left div-buttom-margin">
                        @RenderSection("header", required: false)
                    </div>
                    <div class="col-md-2 pull-right test-clock">
                        <div id="clockbox"></div>
                        <div id="clockboxd"></div>
                    </div>
                    <div class="clearfix"></div>
                    @RenderSection("Subheader", required: false)
                </div>
            </div>
            <!-- /pageheader -->

            <div class="main-content">


                @if (Session["USER_CODE"] != null)
                {
                    if (Session["ROLE_CODE"].ToString() != "P")
                    {
                        Response.Redirect("~/Parent/Logout");
                    }
                    else
                    {
                        @RenderBody()
                    }
                }
                else
                {
                    Response.Redirect("~/Parent/Logout");
                }
                <div class="row">
                    <div class="col-md-12 text-center footer">
                        <p class="designed-by">©2017 Digichamps. All Rights Reserved. |  Designed By <a href="https://www.ntsplhosting.com/">NTSPL</a></p>
                    </div>
                </div>
