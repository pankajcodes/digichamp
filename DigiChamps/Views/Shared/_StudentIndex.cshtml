                    if (element.parent('.input-group').length) {
                        error.insertAfter(element.parent());
                    } else if (element.next('.chosen-container').length) {
                        error.insertAfter(element.next('.chosen-container'));
                    } else {
                        error.insertAfter(element);
                    }
