<form method="post" id="validation-form" class="x-margin">
    <div class="row">
        <div class="col-md-12 questions-cont">
            <!-- Question Panel Starts -->
            <div class="panel panel-ntspl">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        Your Question Details
                    </h3>
                </div>
                <div class="panel-body mail_widget">
                    <div class="list-color ">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 list-color-2">
                                @foreach (var v in ViewBag.question)
                            {

                            if (ViewBag.isclosed == "C" )
                                {
                                     <p><span  class="question-section">Doubt No-</span>@v.Ticket_No (<b style="color:red">CLOSED</b>)</p>
                                    if (ViewBag.isremark!=null)
                                    {
                                         <p><span class="question-section">Remark-</span>@ViewBag.isremark</p>
                                    }
                                   
                                }
                                else if (ViewBag.isclosed == "R")
                                {
                                    <p><span class="question-section red">Doubt No-</span>@v.Ticket_No (<b style="color:red">REJECTED</b>)</p>
                                      if (ViewBag.isremark!=null)
                                    {
                                         <p><span class="question-section">Remark-</span>@ViewBag.isremark</p>
                                    }
                                }
                                else
                                {
                                    <p><span class="question-section">Doubt No-</span>@v.Ticket_No</p>
                                }
                           
                            <p> <span class="question-section">Question- </span>@v.Question</p>
                            <div>
                                @if (@v.Question_Image != null)
                                {
                                <span class="question-section">Question Image- </span>
                                <div class="picture" itemscope itemtype="">
                                    <figure itemprop="associatedMedia" itemscope itemtype="">
                                        <a href="~/Images/Qusetionimages/@v.Question_Image" class="Qc" itemprop="contentUrl" data-size="500x448" data-index="0"><span class="badge bgn-color"><i class="fa fa-link"></i></span></a>
                                    </figure>
                                    <img id="images" src="~/Images/Qusetionimages/@v.Question_Image" hidden>
                                    <script>
                                        getImageSize1($('#images'), function (width, height) {
                                            $(".Qc").attr("data-size", width + 'x' + height);
                                        });
                                        function getImageSize1(img, callback) {
                                            var $img = $(img);

                                            var wait = setInterval(function () {
                                                var w = $img[0].naturalWidth,
                                                    h = $img[0].naturalHeight;
                                                if (w && h) {
                                                    clearInterval(wait);
                                                    callback.apply(this, [w, h]);
                                                }
                                            }, 0);
                                        }
                                    </script>
                                </div>
                                }

                            </div>
                            }

                                @foreach (var v in ViewBag.all_ticketansr)
                            {
                            <p> <span class="question-section">Answer- </span> @v.Answer</p>
                            <div>
                                <input type="hidden" id="Ticket_id" value="@v.Ticket_ID" name="Ticket_id" />
                                <input type="hidden" id="Ticket_answerid" value="@v.Ticket_Dtls_ID" name="Ticket_answerid" />
                                @if (@v.Answer_Image != null)
                                {
                                 
                                <span class="question-section">Answer Image- </span>
                                <div class="picture" itemscope itemtype="">
                                    <figure itemprop="associatedMedia" itemscope itemtype="">
                                        <a href="~/Images/Answerimage/@v.Answer_Image" itemprop="contentUrl" class="ac" data-size="500x448" data-index="0"><span class="badge bgn-color"><i class="fa fa-link"></i></span></a>
                                    </figure>
                                    <img id="ansimg" src="~/Images/Answerimage/@v.Answer_Image" hidden>
                                    <script>
                                        getImageSize2($('#ansimg'), function (width, height) {
                                            $(".ac").attr("data-size", width + 'x' + height);
                                        });
                                        function getImageSize2(img, callback) {
                                            var $img = $(img);

                                            var wait = setInterval(function () {
                                                var w = $img[0].naturalWidth,
                                                    h = $img[0].naturalHeight;
                                                if (w && h) {
                                                    clearInterval(wait);
                                                    callback.apply(this, [w, h]);
                                                }
                                            }, 0);
                                        }
                                    </script>
                                </div>
                                 
                                }
                            </div>
                            }
                               
                            </div>
                        </div>
                       @if ((ViewBag.comments).Count > 0)
                       {
    <div class="panel panel-ntspl inner-panel">
        <div class="mail_list">
            @{
                           DigiChamps.Models.DigiChampsEntities DbContext = new DigiChamps.Models.DigiChampsEntities();
            }

            <script>
                function getImageSize3(img, callback) {
                    var $img = $(img);

                    var wait = setInterval(function () {
                        var w = $img[0].naturalWidth,
                            h = $img[0].naturalHeight;
                        if (w && h) {
                            clearInterval(wait);
                            callback.apply(this, [w, h]);
                        }
                    }, 0);
                }
            </script>
            <ul class="messages messages-stripped messages-zigzag">
                @{int loop = 1;
                       }
                @foreach (var v in ViewBag.comments)
                {
                    <li>
                        <div>
                            @{
                    int id_ = Convert.ToInt32(v.User_Id);

                    var student_name = DbContext.tbl_DC_Registration.Where(x => x.Regd_ID == id_ && x.Is_Active == true & x.Is_Deleted == false).FirstOrDefault();
                    var teacher_name = DbContext.tbl_DC_Teacher.Where(x => x.Teach_ID == id_ ).FirstOrDefault();
                            }
                        </div>
                        @if (student_name != null)
                        {
                            if (student_name.Image != null && v.Is_Teacher==false)
                            {
                                <img src="~/Images/Profile/@student_name.Image" />
                            }
                            else
                            {
                                <img src="~/Images/Profile/no_image.jpg" />
                            }

                        }
                        else
                        {
                            if (teacher_name.Image != null)
                            {
                                <img src="~/Images/Teacherprofile/@teacher_name.Image" />
                            }
                            else
                            {

                                <img src="~/Images/Profile/no_image.jpg" />
                            }

                        }
                        <div>
                            <div>
                                @if (student_name != null && v.Is_Teacher==false)
                                {
                                    <h5>@student_name.Customer_Name (Student)</h5>
                                }
                                else
                                {
                                    <h5>@teacher_name.Teacher_Name (Teacher)</h5>
                                }
                                @{
                    string date = Convert.ToString(@v.User_Comment_Date);
                                }
                                <span class="time"><i class="fa fa-calendar"></i> @date.Substring(0, 9)</span>
                            </div>
                            <p>@v.User_Comment</p>
                            <div>
                                @if (@v.R_image != null)
                                {

                                    <span class="question-section">Attachment- </span>
                                    <div class="picture" itemscope itemtype="">
                                        <figure itemprop="associatedMedia" itemscope itemtype="">
                                            <a href="~/Images/Qusetionimages/@v.R_image" itemprop="contentUrl" id="ans_@loop" data-size="500x448" data-index="0"><span class="badge bgn-color"><i class="fa fa-link"></i></span></a>
                                        </figure>
                                    </div>
                                    <img id="t_@loop" src="~/Images/Qusetionimages/@v.R_image" hidden>
                                    <script>
                                        getImageSize3($('#t_@loop'), function (width, height) {
                                            $('#ans_@loop').attr("data-size", width + 'x' + height);
                                        });
                                    </script>
                                    loop++;
                                }

                            </div>
                        </div>
                    </li>
                }

            </ul>
        </div>
    </div>
}
