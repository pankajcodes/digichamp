    <!--sign in popup start-->
    <div class="s-popup">
        <div class="login-sign-popup-container clearfix">
            <span class="close-popup c-hover"><i class="ti-close"></i></span>
            <div class="sign-in-popup">Sign Up</div>
            <div class="clearfix"></div>
            <form class="form-div" id="sign_up_form">
                <span class="error_alert_message"></span>
                <span class="success_alert_message"></span>
                <div class="pre-pop">
                    <div class="group">
                        <input type="text" class="Mobile" name="mobile" id="sign_mob" maxlength="10" data-rule-required="true" data-rule-minlength="10" data-rule-number="true" data-msg-required="Please enter mobile number">
                        <!--<span class="highlight"></span>-->
                        <!--<span class="bar"></span>-->
                        <label>Enter Your Mobile Number</label>
                    </div>
                    <div class="group">
                        <input type="text" name="name" maxlength="120" onkeypress="return isAlfa(event)" data-rule-required="true" data-msg-required="Please enter name">
                        <!--<span class="highlight"></span>-->
                        <!--<span class="bar"></span>-->
                        <label>Enter Your Name</label>
                    </div>
                    <div class="group">
                        <input type="email" name="email" maxlength="120" data-rule-required="true" data-msg-required="Please enter email id" data-rule-email="true">
                        <!--<span class="highlight"></span>-->
                        <!--<span class="bar"></span>-->
                        <label>Enter Your Email</label>
                    </div>
                    <button class="new-sign-in" type="button" id="next_btn">Next</button>
                    <p class="already">Already have an account with DigiChamps?</p>
                    @*<div class="already-log-in click-login">Forgot Password</div>*@
                    <div class="already-log-in f-color click-login">Log In</div>
                </div>
                <div class="sc-pop" hidden>
                    <span class="sel-gen">You Are a</span>
                    <ul class="list-inline checkboxes-radio">
                        <li>
                            <input type="radio" name="gender" id="c5" value="1" />
                            <label for="c5"><span class="male"></span>Male</label>
                        </li>
                        <li>
                            <input type="radio" name="gender" id="c6" value="0" />
                            <label for="c6"><span class="female"></span>Female</label>
                        </li>
                    </ul>
                    <div class="group">
                        <input type="text" name="txtdob" autocomplete="off" class="datepicker" data-rule-required="true" data-msg-required="Please Enter DOB">
                        <label>Enter Your DOB</label>
                    </div>

                    <button class="new-sign-in" type="submit" id="next_signup">Get Started</button>
                </div>
            </form>
        </div>
    </div>

    <!--sign in popup end-->
    <!--login popup start-->
    <div class="l-popup">
        <div class="login-sign-popup-container clearfix">
