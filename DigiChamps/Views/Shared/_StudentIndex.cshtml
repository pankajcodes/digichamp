                    <!--<span class="highlight"></span>-->
                    <!--<span class="bar"></span>-->
                    <label>Enter Your Mobile Number</label>
                </div>
                <button class="new-sign-in" type="submit">Send OTP</button>
            </form>
        </div>
    </div>
    <!--forgot password popup end-->
    <!--confirm start-->
    <div class="otp-popup">
        <div class="login-sign-popup-container clearfix">
            <span class="close-otp-popup c-hover"><i class="ti-close"></i></span>
            <div class="sign-in-popup">Verification</div>
            <div class="clearfix"></div>
            <form class="form-div" id="otp_form">
                <span class="error_alert_message"></span>
                <span class="success_alert_message"></span>
                <p class="already per-mobile">Enter the 6-digit OTP sent to <span class="number-block">9999999999</span></p>

                <div class="group">
                    <input type="text" name="Mobile_OTP" data-rule-required="true" data-msg-required="Enter OTP" data-rule-minlength="6" maxlength="6">
                    <!--<span class="highlight"></span>-->
                    <!--<span class="bar"></span>-->
                    <label>Enter Your OTP</label>
                </div>

                <div class="group">
                    <input type="password" name="New_Password" maxlength="20" id="New_Password" data-rule-required="true" data-msg-required="Enter new password" data-rule-minlength="6" >
                    <!--<span class="highlight"></span>-->
                    <!--<span class="bar"></span>-->
                    <label>Enter New Password</label>
                </div>

                <div class="group">
                    <input type="password" name="Confirm_Password" maxlength="20" data-rule-required="true" data-rule-minlength="6" data-msg-required="Enter confirm password" data-rule-equalto="#New_Password">
                    <!--<span class="highlight"></span>-->
                    <!--<span class="bar"></span>-->
                    <label>Confirm New Password</label>
                </div>

                <button class="new-sign-in" type="submit">Verify</button>
                <p class="already re-otp">Did not receive OTP? <span class="re-col" id="click-count">Resend</span></p>
            </form>
        </div>
    </div>
