    <script>
        $('.datepicker').datepicker().on('changeDate', function (e) {
            $(this).datepicker('hide');
        });
        $(document).ready(function () {
            setTimeout(function () {
                setActive();
            }, 350);
        });
        function setActive() {

            aObj = document.getElementById('bs-navbar').getElementsByTagName('a');

            for (i = 0; i < aObj.length; i++) {
                // if (document.location.href.indexOf(aObj[i].href) >= 0) {
                if (document.location.href === aObj[i].href) {
                    if ($(aObj[i]).parent().parent().hasClass('nav')) {
                        $(aObj[i]).parent('ul li').addClass('active');

                    }
                    else {
                        $(aObj[i]).parent().parent().parent('li').addClass('active');
                        $(aObj[i]).addClass('active');
                    }
                }
            }
        }
    </script>
