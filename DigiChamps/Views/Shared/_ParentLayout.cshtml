    <script type="text/javascript" src="~/assets/jquery-validation/dist/jquery.validate.min.js"></script>
    <script type="text/javascript" src="~/assets/jquery-validation/dist/additional-methods.min.js"></script>

    <script>
        $(function () {
            let pathParts = window.location.pathname.split("/");
            let yourLink = pathParts[2] // in this example
            switch (yourLink.toUpperCase()) {
                case 'TESTRESULT':
                    $('#mainMenu li a:contains("Test Analysis")').parent().addClass("current");
                    break;
                case 'EXAMTOPICDETAILS':
                    $('#mainMenu li a:contains("Test Analysis")').parent().addClass("current");
                    break;
                case 'ATTENDANCE':
                    $('#mainMenu li a:contains("Attendance Report")').parent().addClass("current");
                    break;
                case 'TIMETABLE':
                    $('#mainMenu li a:contains("TIme Table")').parent().addClass("current");
                    break;
                case 'FEEDBACK':
                    $('#mainMenu li a:contains("Feedback")').parent().addClass("current");
                    break;
                case 'RESULTVIEW':
                    $('#mainMenu li a:contains("Test Analysis")').parent().addClass("current");
                    break;
                default:
                    $('#mainMenu li a:contains("Dashboard")').parent().addClass("current");
                    // if yourLink is empty then default to the home page
            }
        });
    </script>
