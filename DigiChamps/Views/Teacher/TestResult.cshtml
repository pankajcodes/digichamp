@{
    ViewBag.Title = "TestResult";
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}


<form id="Ticke_form" class="form-horizontal">
    <div class="page-content-wrap">
        <div class="row">
            <div class="col-md-12">
                <div class="box">
                    <div class="box-title">
                        <h3><i class="fa fa-table"></i>Test Result</h3>
                        
                    </div>
                    <div class="box-content">
                        <div class="btn-toolbar pull-right clearfix">
                            @* <div class="btn-group">
                                                            <a class="btn btn-circle show-tooltip" title="Refresh" href="/Teacher/Viewticekts"><i class="fa fa-refresh"></i></a>
                                    <a class="btn btn-circle show-tooltip" title="My tickets" href="/Teacher/mytickets"><i class="fa fa-filter"></i></a>
                                </div>*@
                        </div>
                        <br /><br />
                        <div class="clearfix"></div>
                        <div class="table-responsive" style="border:0">
                            <table class="table table-advance table-hover" id="table1">
                                <thead>
                                    <tr>
                                        <th hidden>Sl no</th>                                     
                                        <th>Student Name</th> 
                                        <th>Batch Name</th>                                                                          
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (ViewBag.test_result != null)
                                    {
                                        int i = 0;
                                        foreach (var item in ViewBag.test_result)
                                        {
                                            <tr>
                                                <td hidden>@(i += 1)</td>                                              
                                                <td>@item.Customer_Name</td>
                                                <td>@item.Batch_Name</td>                                                                              
                                                <td>                                           
                                                    @Html.ActionLink(" ", "ViewResultDetail", "Teacher", new { id = @item.Regd_ID }, new { @class = "btn btn-sm btn-primary show-tooltip fa fa-eye", @title = "View Result Detail" })
                                                </td>
                                            </tr>
                                        }
                                    }
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>
</form>
